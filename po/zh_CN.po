msgid "ArrowMoveSize"
msgstr "方向键移动尺寸"

#
#: noprefs.c:230 prefs.c:256
msgid "The number of em-units by which an arrow key will move a selected point"
msgstr "方向键按此尺寸移动选定点"

#: noprefs.c:231 prefs.c:257
msgid "ArrowAccelFactor"
msgstr "方向键加速系数"

#: noprefs.c:231 prefs.c:257
msgid ""
