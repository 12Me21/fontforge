# Support for --with-gnustep-make

CC       = @CC@
CPPFLAGS = @CPPFLAGS@
CFLAGS   = @CFLAGS@
LDFLAGS  = @LDFLAGS@

ADDITIONAL_INCLUDE_DIRS = -I@abs_top_srcdir@/inc
ADDITIONAL_CFLAGS = $(CPPFLAGS) $(CFLAGS) @MY_CFLAGS@ @PTHREAD_CFLAGS@ -DHAVE_CONFIG_H
ADDITIONAL_LDFLAGS = @MY_CFLAGS@ $(LDFLAGS) @LIBS@ @MY_LIBS@ @PTHREAD_LIBS@
